{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0",
    "prebuild": "node ./scripts/generate-sitemap.mjs",
    "build": "vite build",
    "preview": "vite preview",
    "serve": "serve -s dist -l 5173",
    "lint": "eslint .",
    "lint:a11y": "eslint src --ext .jsx --rule 'jsx-a11y/*: 2'",
    "test": "vitest --environment jsdom --watch=false",
    "test:coverage": "vitest run --environment jsdom --coverage --coverage.reportsDirectory=coverage",
    "seo:audit": "npx lighthouse http://localhost:5173 --only-categories=seo,accessibility --quiet --output=json --output-path=./lighthouse-report.json"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "date-fns": "^4.1.0",
    "jwt-decode": "^4.0.0",
    "lucide-react": "^0.546.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-helmet-async": "^2.0.5",
    "react-hot-toast": "^2.6.0",
    "react-router-dom": "^6.30.1"
  },
  "devDependencies": {
    "@axe-core/react": "^4.11.0",
    "@eslint/js": "^9.36.0",
    "@tailwindcss/postcss": "^4.1.14",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "@vitest/coverage-v8": "^4.0.3",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.36.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-vitest": "^0.5.4",
    "globals": "^16.4.0",
    "jsdom": "^27.0.1",
    "msw": "^2.11.6",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.14",
    "vite": "^7.1.7",
    "vite-plugin-pwa": "^1.1.0",
    "vitest": "^4.0.3"
  }
}
